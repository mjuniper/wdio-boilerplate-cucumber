{
  "name": "wdio-boilerplate-cucumber",
  "version": "2.0.3",
  "description": "Everything you need to get started with selenium tests in node.js.",
  "repository": {
    "type": "git",
    "url": "https://github.com/dcypherthis/wdio-boilerplate-cucumber.git"
  },
  "main": "wdio.conf.js",
  "scripts": {
    "postinstall": "node_modules/.bin/selenium-standalone install",
    "lint": "node_modules/.bin/eslint test/**/*.js",
    "test": "node_modules/.bin/wdio test/config/prod.conf.js",
    "selenium": "node_modules/.bin/selenium-standalone start",
    "report": "node_modules/.bin/allure generate test/reports/allure-results/ && allure report",
    "test-local-dev": "wdio test/config/suite.conf.js"
  },
  "keywords": [
    "test",
    "automation",
    "selenium",
    "webdriver",
    "webdriverIO"
  ],
  "author": "Josh Cypher <joshua.cypher@me.com>",
  "license": "MIT",
    "dependencies": {
        "babel-preset-es2015": "6.3.13",
        "babel-preset-stage-0": "^6.3.13",
        "babel-register": "^6.3.13",
        "chai": "3.5.0",
        "chai-as-promised": "5.3.0",
        "cucumber": "^0.10.4",
        "node-uuid": "^1.4.7",
        "wdio-cucumber-framework": "0.1.0",
        "webdriverio": "4.0.9",
        "wdio-allure-reporter": "0.0.2",
        "wdio-dot-reporter": "0.0.5",
        "wdio-junit-reporter": "0.0.2",
        "wdio-sauce-service": "^0.2.2"
    },
    "devDependencies": {
        "allure-commandline": "^1.4.22-1",
        "eslint": "^1.10.3",
        "eslint-plugin-react": "^3.16.1",
        "node-uuid": "^1.4.7",
        "selenium-standalone": "^5.1.1"
    }
}
